{
    "family": "nodeapp1-test-yml-td",
    "containerDefinitions": [
        {
            "name": "nodeapp1container",
            "image": "DYNAMIC_TO_ACTIONS",
            "portMappings": [
                {
                    "name": "nodeapp1container-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "networkMode": "bridge",
    "memory": "0.2 GB",
    "cpu": "0.2 vCPU"
}